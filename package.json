{
  "name": "unplugin-auto-export",
  "version": "1.0.3",
  "packageManager": "pnpm@8.6.12",
  "description": "Automates the maintenance of export statements in the index.ts file.",
  "author": "coderhyh <coderhyh@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/coderhyh/unplugin-auto-export#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderhyh/unplugin-auto-export.git"
  },
  "bugs": {
    "url": "https://github.com/coderhyh/unplugin-auto-export/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@nuxt/kit": "^3.6.5",
    "@nuxt/schema": "^3.6.5",
    "@types/node": "^20.5.2",
    "bumpp": "^9.2.0",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "webpack": "^5.88.2"
  }
}
